{
  "expo": {
    "name": "Unaadeb Play",
    "slug": "unaadebplay",
    "version": "1.0.9",
    "scheme": "unaadebplay",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "owner": "thsdeveloper",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E51C44"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The $(PRODUCT_NAME) app accesses your photos to allow you to share them on your profile."
        }
      ],
      [
        "expo-updates",
        {
          "username": "thsdeveloper"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "7a5a2114-b085-4afe-b2c7-a3cfec041131"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7a5a2114-b085-4afe-b2c7-a3cfec041131"
    },
    "android": {
      "package": "com.unaadebplay",
      "versionCode": 7
    },
    "ios": {
      "bundleIdentifier": "com.unaadebplay",
      "buildNumber": "1.0.3"
    }
  }
}
